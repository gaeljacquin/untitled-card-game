{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "web",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "tags": ["scope:web", "type:app"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dev",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm build",
        "cwd": "apps/web"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm start",
        "cwd": "apps/web"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint",
        "cwd": "apps/web"
      }
    },
    "lint-fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint-fix",
        "cwd": "apps/web"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm typecheck",
        "cwd": "apps/web"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "apps/web"
      }
    },
    "prettier-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm prettier:check",
        "cwd": "apps/web"
      }
    },
    "prettier-write": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm prettier:write",
        "cwd": "apps/web"
      }
    }
  }
}
